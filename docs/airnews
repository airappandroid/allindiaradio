 <style>
.filter-wrap{
  display:flex;
  gap:10px;
  margin-bottom:15px;
  flex-wrap:wrap;
}
.filter-wrap input,
.filter-wrap select{
  padding:8px 10px;
  border:1px solid #ddd;
  border-radius:4px;
  font-size:14px;
}
.filter-wrap input{
  flex:1;
  min-width:180px;
}
</style>

<div class="filter-wrap">
  <input type="text" id="searchBox" placeholder="Search city or languageâ€¦">

  <select id="cityFilter">
    <option value="all">All Cities</option>
  </select>

  <select id="languageFilter">
    <option value="all">All Languages</option>
  </select>
</div>

<script>
document.addEventListener("DOMContentLoaded", function () {

  const searchBox = document.getElementById("searchBox");
  const citySelect = document.getElementById("cityFilter");
  const langSelect = document.getElementById("languageFilter");
  const cards = document.querySelectorAll(".bulletin-card");

  let cities = new Set();
  let langs = new Set();

  cards.forEach(card => {
    cities.add(card.querySelector(".bulletin-city").innerText.trim());
    langs.add(card.querySelector(".lang").innerText.trim());
  });

  cities.forEach(city => {
    citySelect.innerHTML += `<option value="${city}">${city}</option>`;
  });

  langs.forEach(lang => {
    langSelect.innerHTML += `<option value="${lang}">${lang}</option>`;
  });

  function filterCards() {
    const searchVal = searchBox.value.toLowerCase();
    const cityVal = citySelect.value;
    const langVal = langSelect.value;

    cards.forEach(card => {
      const city = card.querySelector(".bulletin-city").innerText.toLowerCase();
      const lang = card.querySelector(".lang").innerText.toLowerCase();

      const matchSearch = city.includes(searchVal) || lang.includes(searchVal);
      const matchCity = cityVal === "all" || card.querySelector(".bulletin-city").innerText === cityVal;
      const matchLang = langVal === "all" || card.querySelector(".lang").innerText === langVal;

      card.style.display = (matchSearch && matchCity && matchLang) ? "block" : "none";
    });
  }

  searchBox.addEventListener("keyup", filterCards);
  citySelect.addEventListener("change", filterCards);
  langSelect.addEventListener("change", filterCards);

});
</script>

						 
